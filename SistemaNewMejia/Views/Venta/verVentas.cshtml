@{
    ViewBag.Title = "Ventas Realizadas";
}

<h1>Ventas</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">N°</th>
            <th scope="col">Fecha</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in (List<Venta>)Model)
        {
            <tr>
                <td>@i.Id</td>
                <td>@i.Fecha</td>
            </tr>
        }
    </tbody>
</table>